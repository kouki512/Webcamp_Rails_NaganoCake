
<h1>Editing Genre</h1>
<%= form_with model: @genre, url: admin_genre_path, method: :patch, local:true do |f| %>
<table>
    <tbody>
        <tr>
            <td><%= f.label "ジャンル名" %></td>
            <td><%= f.text_field :name %></td>
            <td><%= f.submit "変更を保存" %> </td>
        </tr>
        
    </tbody>
</table>
<% end %>